{
  "name": "@nostr-fetch/examples",
  "version": "0.15.1",
  "description": "nostr-fetch examples",
  "type": "module",
  "private": true,
  "scripts": {
    "check-type": "tsc -p tsconfig.lint.json || exit 0",
    "lint": "run-p check-type lint:*",
    "lint:format": "biome format ./src",
    "lint:js": "biome lint ./src",
    "fix": "run-s fix:*",
    "fix:format": "biome format --write ./src",
    "fix:js": "biome lint --apply ./src",
    "tsx": "tsx"
  },
  "dependencies": {
    "@nostr-dev-kit/ndk": "^2.6.1",
    "@nostr-fetch/adapter-ndk": "^0.15.1",
    "@nostr-fetch/adapter-nostr-relaypool": "^0.15.1",
    "@nostr-fetch/adapter-nostr-tools": "^0.15.1",
    "@nostr-fetch/adapter-nostr-tools-v2": "^0.15.1",
    "@nostr-fetch/adapter-rx-nostr": "^0.15.1",
    "nostr-fetch": "^0.15.1",
    "nostr-relaypool": "^0.6.28",
    "nostr-tools": "^1.13.1",
    "nostr-tools-v2": "npm:nostr-tools@^2.1.0",
    "rx-nostr": "^1.2.0",
    "rxjs": "^7.8.0",
    "websocket-polyfill": "^0.0.3",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10"
  }
}
